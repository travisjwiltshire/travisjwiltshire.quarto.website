@article{van_der_lee_neural_2023,
	title = {Neural {Data}-to-{Text} {Generation} {Based} on {Small} {Datasets}: {Comparing} the {Added} {Value} of {Two} {Semi}-{Supervised} {Learning} {Approaches} on {Top} of a {Large} {Language} {Model}},
