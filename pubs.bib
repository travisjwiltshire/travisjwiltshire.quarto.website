	year = {2023},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/tops.12685},
	keywords = {Dynamical systems, Recurrence quantification analysis, Team adaptation, Team cognition, Team coordination, Team psychophysiology},
	pages = {1--19},
	file = {Full Text PDF:C\:\\Users\\wiltshir\\Zotero\\storage\\74FJKGJE\\van Eijndhoven et al. - A Methodological Framework to Study Change in Team.pdf:application/pdf;Snapshot:C\:\\Users\\wiltshir\\Zotero\\storage\\6EWZMXX9\\tops.html:text/html},
