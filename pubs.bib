Publisher: Educational Publishing Foundation},
	keywords = {Collaboration, Improvisation, Intention, Music, Music Perception, Musicians, Perceptual Motor Coordination},
	pages = {26--39},
	file = {Snapshot:C\:\\Users\\wiltshir\\Zotero\\storage\\6EVK52V5\\2023-95288-001.html:text/html;Submitted Version:C\:\\Users\\wiltshir\\Zotero\\storage\\TF3M6Z8C\\Vaarten and Wiltshire - 2023 - Capturing coordination and intentionality in joint.pdf:application/pdf},
