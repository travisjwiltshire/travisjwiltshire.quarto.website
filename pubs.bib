	keywords = {Aviation, Electroencephalography (EEG), Engagement, Flight training, Functional near-infrared spectroscopy (fNIRS), Mental fatigue, Multimodality, Neurophysiology, Simulation fidelity, Social dynamics, Workload},
	pages = {103838},
}

@article{habaraduwa_implicit_2025,
	title = {Implicit {Coordination} {Dynamics}: {A} {Synchrony}-{Based} {Study} on {Team} {Positioning} and {Performance} in {Competitive} {Dota} 2},
	volume = {17},
	issn = {2475-1510},
	shorttitle = {Implicit {Coordination} {Dynamics}},
	url = {https://ieeexplore.ieee.org/document/10738480},
	doi = {10.1109/TG.2024.3488114},
	abstract = {The collective performance displayed by groups or teams, whether in solving complex problems or excelling in esports competitions, hinges on their coordination dynamics. While explicit coordination (e.g., verbal commands) and its affects on collective outcomes have been studied extensively, implicit coordination, especially in dynamic, fast-paced environments have been under investigated. In this study, we examine the competitive esport Dota 2 (D2) as a setting to explore within-team implicit coordination, which we model as player avatar movement synchrony. Utilizing the cluster phase method, we analyze spatio-temporal patterns of player movements to quantify implicit movement coordination. We observe a negative linear relationship between team movement synchrony and team performance in rank for D2 competitions across two tournaments. While some research suggests stronger coordination leads to favorable outcomes, we leverage our findings to discuss the complexity of team coordination, showcasing a delicate balance between specialization of individual team members and collective action. This study not only extends complex systems techniques used in physical sports to the rapidly evolving esports arena, but also invites further exploration into the multidimensional nature of coordination in team-based activities.},
	number = {2},
	urldate = {2025-06-19},
	journal = {IEEE Transactions on Games},
	author = {Habaraduwa, Udesh and Blom, Paris Mavromoustakos and Wiltshire, Travis J.},
	month = jun,
	year = {2025},
	keywords = {Avatars, Complex systems, computer games, esports, Games, Motion measurement, Observers, Pattern matching, Position measurement, Real-time systems, Sports, Synchronization, team dynamics},
	pages = {442--450},
	file = {Snapshot:C\:\\Users\\wiltshir\\Zotero\\storage\\CFWHKSFI\\10738480.html:text/html;Submitted Version:C\:\\Users\\wiltshir\\Zotero\\storage\\LTZSEZ5Y\\Habaraduwa et al. - 2025 - Implicit Coordination Dynamics A Synchrony-Based .pdf:application/pdf},
