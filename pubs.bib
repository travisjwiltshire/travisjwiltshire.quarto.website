@article{kraeter_effect_2022,
	title = {Effect of stimuli congruency on gaze behavior and memory},
	volume = {44},
	url = {https://escholarship.org/uc/item/2j87q9kf},
	abstract = {We investigated whether schema congruency differentially affects low level sensory processing (eye gaze) compared to higher-level cognition (memory). Participants performed a two-phase eye tracking task; first a baseline phase with only congruent cartoon events, and subsequently an experimental phase in which the same events were adapted to remain congruent or become incongruent to a theme. Results revealed that participants became quicker in recognizing the congruent cartoon events compared to incongruent in the experimental phase, indicating improved memory for congruent cartoon events. No mean difference in gaze towards congruent versus incongruent events was observed. Surprisingly, a slight bias towards gazing to the left side of the screen in the baseline phase diminished during the experimental phase, indicating that the schema congruency manipulation might affect gaze behavior. Taken together, our results suggest that our schema congruency manipulation affects gaze behavior and memory, but further eye tracking analysis could reveal the dynamic nature of this effect.},
	language = {en},
	number = {44},
	urldate = {2025-10-29},
	journal = {Proceedings of the Annual Meeting of the Cognitive Science Society},
	author = {Kraeter, Vivienne and Kritskaia, Veronika and Wiltshire, Travis J. and Collin, Silvy H. P.},
	year = {2022},
}

