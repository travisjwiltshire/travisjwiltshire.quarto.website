	keywords = {Cognitive science, Human-computer interaction, Legged locomotion, machine learning, Machine learning algorithms, navigation, Navigation, Prediction algorithms, redirected walking, Systematics, Task analysis, virtual reality},
	pages = {5765--5781},
	file = {Full Text PDF:C\:\\Users\\wiltshir\\Zotero\\storage\\LT37JFSK\\Croucher et al. - 2024 - LoCoMoTe – A Framework for Classification of Natur.pdf:application/pdf},
